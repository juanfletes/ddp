<p id="notice"><%= notice %></p>

<h1>Circulo Amistades</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Direccion</th>
      <th>Horario</th>
      <th>Pasivo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @circulo_amistades.each do |circulo_amistad| %>
      <tr>
        <td><%= circulo_amistad.nombre %></td>
        <td><%= circulo_amistad.direccion %></td>
        <td><%= circulo_amistad.horario %></td>
        <td><%= circulo_amistad.pasivo %></td>
        <td><%= link_to 'Show', circulo_amistad %></td>
        <td><%= link_to 'Edit', edit_circulo_amistad_path(circulo_amistad) %></td>
        <td><%= link_to 'Destroy', circulo_amistad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Circulo Amistad', new_circulo_amistad_path %>
